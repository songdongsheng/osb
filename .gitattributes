*.csproj        text eol=crlf
*.rc            text eol=crlf
*.reg           text eol=crlf
*.sln           text eol=crlf
*.sql           text eol=crlf
*.vcxproj       text eol=crlf

*.c             text eol=lf
*.cpp           text eol=lf
*.css           text eol=lf
